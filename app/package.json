{
  "scripts": {
    "dev": "node -r ts-node/register",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/?(*.)(spec|test).[jt]s?(x)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphene-js": "^0.5.2",
    "graphene-sequelize": "^0.1.0",
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0",
    "sqlite3": "^4.0.4",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@types/cookie-session": "^2.0.36",
    "jest": "^23.6.0"
  }
}
